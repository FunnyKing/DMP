openapi: 3.0.2
info:
  title: 七河生物-研发模块 - SAAS
  version: 1.0.50.beta 
servers:
  - url: "http://jiuxiniot.com/qihe-biology-e"
tags:
  - name: 原材料
  - name: 堆料实验
  - name: 取用记录
  - name: 单核菌株
  - name: 配方
  - name: 库房
  - name: 培养基
  - name: 母种-一级母种
  - name: 母种-二级母种
  - name: 母种-菌种检测
  - name: 母种-生产菌棒检测
  - name: 种质入库
  - name: 群体信息
  - name: 实验
paths:
  /material/list:
    get:
      tags:
        - 原材料
      summary: 列表（分页）
      security:
        - token: []
      parameters:
        - name: name
          in: query
          required: false
          description: 材料名称
          schema:
            type: string
        - $ref: "#/components/parameters/PageNum"
        - $ref: "#/components/parameters/PageSize"
      responses:
        '200':
          description: ""
          content:
            application/json:
              schema:
                allOf:
                  - $ref: "#/components/schemas/ResponseEntity"
                  - type: object
                    properties:
                      data:
                        allOf:
                          - $ref: "#/components/schemas/PageInfo"
                          - type: object
                            properties:
                              list:
                                type: array
                                items:
                                  properties:
                                    id:
                                      type: string
                                      description: id
                                    name:
                                      type: string
                                      description: 材料名称
                                    batchId:
                                      type: string
                                      description: 批次
                                    discoverTime:
                                      type: string
                                      description: 检测日期，yyyy-MM-dd HH:mm:ss
  /material/{id}:
    get:
      tags:
        - 原材料
      summary: 详情
      security:
        - token: []
      parameters:
        - name: id
          in: path
          required: true
          description: 材料名称
          schema:
            type: string
      responses:
        '200':
          description: ""
          content:
            application/json:
              schema:
                allOf:
                  - $ref: "#/components/schemas/ResponseEntity"
                  - type: object
                    properties:
                      data:
                        allOf:
                          - $ref: "#/components/schemas/Material"
  /material:
    post:
      tags:
        - 原材料
      summary: 保存
      security:
        - token: []
      requestBody:
        required: true
        description: 原材料的实体类，保存时不需要传递id
        content:
          application/json:
            schema:
              $ref: "#/components/schemas/Material"
      responses:
        '200':
          description: ""
          content:
            application/json:
              schema:
                allOf:
                  - $ref: "#/components/schemas/ResponseEntity"
                  - type: object
                    properties:
                      data:
                        allOf:
                          - $ref: "#/components/schemas/Material"
    put:
      tags:
        - 原材料
      summary: 修改
      security:
        - token: []
      requestBody:
        required: true
        description: 原材料的实体类
        content:
          application/json:
            schema:
              $ref: "#/components/schemas/Material"
      responses:
        '200':
          description: ""
          content:
            application/json:
              schema:
                allOf:
