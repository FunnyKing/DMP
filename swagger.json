        - 堆料实验
      summary: 保存
      security:
        - token: []
      requestBody:
        required: true
        content:
          application/json:
            schema:
              required:
                - name
                - createUser
              type: object
              properties:
                name:
                  type: string
                  description: 材料名称
                createUser:
